@article{Luft2015,
 abstract = {Der Artikel beschäftigt sich mit der Gegenüberstellung von angewandten Wissenschaften und Grundlagenwissenschaften, er geht dabei auf die Vor- und Nachteile der beiden Wissenschaft ein und erklärt diese praxisbezogen an Beispielen. Die Gegenüberstellung ermöglicht dem Leser Informationswissenschaften als Wissenschaft einordnen zu können und gibt Aufschluss darüber, wieso Informationswissenschaften eine angewandte Wissenschaft ist. Weiterhin wird erläutert, dass sich eine Mischung aus beiden Wissenschaften als Beste erweist, um ein möglichst gutes Forschungsergebnis zu erzielen. Zudem wird Informationswissenschaft als essenzielle Wissenschaft vorgestellt und ihr hoher Stellenwert wird anhand von Beispielen beschrieben.},
 author = {Luft, Joan},
 year = {2015},
 title = {The Challenges of Being a Fox: Library and Information Science as an Applied Discipline},
